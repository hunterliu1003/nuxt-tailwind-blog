{
  "name": "nuxt-tailwind-blog",
  "version": "1.0.0",
  "description": "My splendiferous Nuxt.js project",
  "author": "Hunter",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build --modern=server",
    "analyze": "nuxt build --modern=server --analyze",
    "start": "nuxt start --modern=server",
    "generate": "nuxt generate --modern",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "lint-staged": {
    "*.{js,vue}": "yarn lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "express": "^4.16.4",
    "nuxt": "2.12.2",
    "nuxt-compress": "^1.1.0"
  },
  "devDependencies": {
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/markdownit": "^1.2.9",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.2.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@nuxtjs/tailwindcss": "^1.4.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "markdown-it-attrs": "^3.0.2",
    "node-zopfli-es": "^1.0.4",
    "nodemon": "^1.18.9",
    "prettier": "^1.19.1",
    "pug": "^2.0.4",
    "pug-plain-loader": "^1.0.0",
    "shrink-ray-current": "^4.1.2",
    "stylelint": "^10.1.0",
    "vue-automatic-import-loader": "^0.1.3"
  }
}
